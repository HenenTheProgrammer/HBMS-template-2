<!--forEach[latest_posts]-->
<article class="post">
    <a href="read/<!--{URI}-->">
        <div class="header-cover">
            <img src="<!--{FeatureImage}-->" alt="<!--{Title}-->" width="100"/>
            <div class="content">
                <time><!--{PostTime}--></time>
                <ul class="labels">
                    <!--{Labels}-->
                </ul>
            </div>
        </div>
        <div class="main">
            <h3><!--{Title}--></h3>
            <p><!--{Content}--></p>
            <b>Read post <i class="fa fa-arrow-right"></i> </b>
        </div>
    </a>
</article>
 <!--end[latest_posts]-->